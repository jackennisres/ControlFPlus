{
  "manifest_version":2,
  "name": "Control F Plus",
  "description": "Find things quicker using multiple queries at once.",
  "version": "0.7",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches":[
        "<all_urls>"
      ],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
